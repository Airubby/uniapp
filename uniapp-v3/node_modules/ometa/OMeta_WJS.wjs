  M = OMeta.delegated({
    number: function() {
      var $elf = this
      return $elf._or(
        function() {
          var n, d
          n = $elf._apply("number")
          d = $elf._apply("digit")
          return n * 10 + d.digitValue()
        },
        function() {
          var d
          d = $elf._apply("digit")
          return d.digitValue()
