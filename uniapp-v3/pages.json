			{
				"path": "carTime",
				"style": {
					"app-plus": {
						"titleNView": {
							"buttons": [{
								"text": "取消",
								// "fontSrc": "/static/iconfont.ttf",
								"fontSize": "16px",
								"color": "#ffffff",
