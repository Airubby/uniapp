    "@dcloudio/uni-cli-shared": "^0.2.994",
    "@dcloudio/vue-cli-plugin-uni": "^0.9.538",
    "@dcloudio/webpack-uni-mp-loader": "^0.3.644",
    "@dcloudio/webpack-uni-pages-loader": "^0.2.875",
    "@vue/cli-plugin-babel": "3.1.1",
